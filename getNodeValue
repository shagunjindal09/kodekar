
int getNode(SinglyLinkedListNode* llist, int positionFromTail) {
    int c = 1, i = 1;
        SinglyLinkedListNode *ptr = llist;
        while(ptr->next != NULL){
            c += 1;
            ptr = ptr->next;
        }
        if(positionFromTail == 0)
            return ptr->data;
        ptr = llist;
        c = c - positionFromTail;
        while(i != c){
            ptr = ptr->next;
            i += 1;
        }
        return ptr->data;

}
